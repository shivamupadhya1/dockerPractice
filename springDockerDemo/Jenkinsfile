pipeline {
     tools {
        maven 'Maven 3.9.9' // Make sure this matches the name you gave in Global Tool Configuration
    }

    stages { // Add this block to contain all the stages
        stage('Build') {
            steps {
                // Build the project using Maven
                script {
                    bat 'mvn clean install -Ddependency-check.skip=true' // Use 'bat' if on Windows
                }
            }
        }
    }
}
